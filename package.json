{
  "name": "birthday-wish",
  "version": "1.0.0",
  "description": "Birthday wish webpage using Vue 3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "vite build && gh-pages -d dist --dotfiles",
    "optimize-images": "node optimize-images.js",
    "generate-configs": "node scripts/generate-configs.js",
    "update-memories": "node scripts/update-memories.js",
    "update-memory-configs": "node scripts/update-memory-configs.js",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "element-plus": "^2.13.0",
    "gsap": "^3.14.2",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.20.1",
    "gh-pages": "^6.3.0",
    "prettier": "^3.2.5",
    "sass": "^1.97.1",
    "sharp": "^0.34.5",
    "terser": "^5.44.1",
    "vite": "^5.0.0",
    "vite-plugin-imagemin": "^0.6.1"
  }
}
